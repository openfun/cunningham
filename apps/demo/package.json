    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@vitejs/plugin-react": "3.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.57.1",
    "typescript": "4.9.4",
    "vite": "4.0.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
