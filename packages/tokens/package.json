{
  "name": "cunningham-tokens",
  "private": false,
  "version": "0.0.5",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "cunningham": "./dist/cunningham.js"
  },
  "files": [
    "./dist/"
  ],
  "scripts": {
    "lint": "eslint . 'src/**/*.ts?(x)'",
    "dev": "nodemon --watch src --ext '*' --exec npm run build",
    "dev-bin": "nodemon --watch 'src/bin' --ext '*' --exec 'yarn build-bin && yarn build-theme'",
    "build": "tsc && vite build",
    "build-bin": "tsc -p src/bin/tsconfig.json && chmod +x dist/cunningham.js",
    "build-theme": "cunningham"
  },
  "dependencies": {
    "@types/figlet": "^1.5.5",
    "@types/node": "^18.11.9",
    "chalk": "^5.1.2",
    "eslint-config-custom": "*",
    "figlet": "^1.5.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4",
    "vite": "^3.2.3",
    "vite-plugin-dts": "^1.7.1"
  },
  "devDependencies": {
    "rollup-plugin-hashbang": "^3.0.0"
  }
}
