{
  "name": "cunningham",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "test-ci": "turbo run test-ci",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,cjs,js}\""
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "make-dir-cli": "3.1.0",
    "prettier": "3.1.1",
    "turbo": "1.11.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@changesets/cli": "2.27.1"
  },
  "packageManager": "yarn@1.22.21",
  "resolutions": {
    "@testing-library/dom": "9.3.3"
  },
  "volta": {
    "node": "18.18.2",
    "yarn": "1.22.21"
  }
}
