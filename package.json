{
  "name": "cunningham",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "test-ci": "turbo run test-ci",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,cjs,js}\""
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "make-dir-cli": "3.1.0",
    "prettier": "3.0.2",
    "turbo": "1.10.12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@changesets/cli": "2.26.2"
  },
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "18.17.1",
    "yarn": "1.22.19"
  }
}
