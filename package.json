{
  "name": "cunningham",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "2.8.8",
    "turbo": "1.9.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@changesets/cli": "2.26.1"
  },
  "packageManager": "yarn@1.22.19",
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  }
}
